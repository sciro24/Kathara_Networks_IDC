<?xml version="1.0" encoding="utf-8"?>
<lab name="SMILE">
  <routers>
    <router name="rxa4">
      <protocols>
        <protocol>bgp</protocol>
      </protocols>
      <asn>7</asn>
      <interfaces>
        <interface name="eth0" lan="X" ip="7.7.7.7/25"/>
        <interface name="eth1" lan="A" ip="50.7.1.7/24"/>
      </interfaces>
    </router>
    <router name="rdy8">
      <protocols>
