    </router>
    <router name="rbc2">
      <protocols>
        <protocol>rip</protocol>
      </protocols>
      <interfaces>
        <interface name="eth0" lan="B" ip="100.1.2.2/24"/>
        <interface name="eth1" lan="C" ip="100.2.3.2/24"/>
      </interfaces>
    </router>
    <router name="rcdf3">
