<?xml version="1.0" encoding="utf-8"?>
<lab name="testFunc">
  <routers>
    <router name="r1">
      <protocols>
        <protocol>bgp</protocol>
      </protocols>
      <asn>1</asn>
      <interfaces>
        <interface name="eth0" lan="A" ip="10.0.0.1/24"/>
      </interfaces>
    </router>
    <router name="r2">
      <protocols>
        <protocol>rip</protocol>
      </protocols>
      <interfaces>
        <interface name="eth0" lan="A" ip="10.0.0.2/24"/>
      </interfaces>
    </router>
    <router name="r3">
      <protocols>
        <protocol>ospf</protocol>
      </protocols>
      <interfaces>
        <interface name="eth0" lan="B" ip="10.1.0.1/24"/>
      </interfaces>
    </router>
  </routers>
  <hosts>
    <host name="host1" ip="10.1.0.2/24" gateway="10.1.0.1/24" lan="B"/>
  </hosts>
  <www>
    <server name="www1" ip="10.2.0.1/24" gateway="10.1.0.1/24" lan="B"/>
  </www>
  <lab_conf>r1[0]=A
r1[image]="kathara/frr"

r2[0]=A
r2[image]="kathara/frr"

r3[0]=B
r3[image]="kathara/frr"

host1[0]=B
host1[image]="kathara/base"

www1[0]=B
www1[image]="kathara/base"
</lab_conf>
</lab>
